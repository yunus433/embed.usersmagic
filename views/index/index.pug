extends ../partials/head
block content
  .add-new-item-outer-wrapper#create-target-group-wrapper(style="display: none")
    form.add-new-item-wrapper
      .add-new-item-close-button#create-target-group-close-button
        i.fas.fa-times
        span Close
      span.all-title Create Target Group
      span.all-input-subtitle Target groups allow you to choose a spesific section of your users using our question database.
      span.all-input-title.required-input Target Group Name
      input.all-input#target-group-name-input(type="text" placeholder="ex. Female 20-22 Basketball players" autocomplete="none" required)
      span.all-input-title.required-input Filters
      .target-group-filters-wrapper
        span.no-filter-text You have added no filters yet
      input.search-filter-input#search-filter-input(type="text" placeholder="Search filters" autocomplete="none")
      .target-group-filters-search-wrapper
        each filter in filters
          .each-target-group-filter
            span.each-target-group-filter-name= filter.name
            if filter.subtype == 'number'
              span.each-target-group-filter-allowed-title Min value (inclusive):
              input.all-input(type="text" placeholder="ex. 20")
              span.each-target-group-filter-allowed-title Max value (inclusive):
              input.all-input(type="text" placeholder="ex. 22")
              span.each-target-group-filter-add.numeric-filter Add
            else
              span.each-target-group-filter-allowed-title Allowed answers:
              each choice in filter.choices
                .each-target-group-filter-choice
                  .each-target-group-filter-choice-check
                    i.fas.fa-check
                  span= choice
              span.each-target-group-filter-add Add
      button.all-confirm-button Create
  input.display-none#pie-chart-colors(type="text" value=JSON.stringify(pie_chart_colors))
  span.copy-data#copy-data
    span= '<script src= "https://embed.usersmagic.com/js/embed/connection.js"></script> <link rel="stylesheet" href="https://embed.usersmagic.com/css/embed/styling.css"></link>'
  .all-wrapper
    .all-header-wrapper
      a(href="/")
        svg(width="12" height="21" viewBox="0 0 12 21" fill="none" xmlns="http://www.w3.org/2000/svg")
